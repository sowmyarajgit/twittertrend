pipeline {
    agent{
        node{
            label "slave"
        }
    }
    
    environment {
         PATH = "/opt/apache-maven-3.9.0/bin/:$PATH"
    }

    stages {
        stage('gitclone') {
            steps {
              git branch: 'main', credentialsId: 'slave-pvt-key', url: 'https://github.com/sowmyarajgit/twittertrend.git'  
            }
        }
        stage('build'){
            
            steps{
            sh 'mvn clean install'
        }        
        }
    }
    
    }
